#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int arr1[1]={0},arr2[1]={0};
    while(n--)
    {   long int a,b;cin>>a>>b;
        if(a<b)
            arr1[0]++;
        if(a>b);
        arr2[0]++;
    }
    if(arr1[0] && arr2[0])
        cout<<"Happy Alex";
    else
        cout<<"Poor Alex";
}
