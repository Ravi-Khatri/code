#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,k;cin>>n>>k;
    long int count=0;
    vector< int >v(n);
    for(int i=0;i<n;i++)
    cin>>v[i];
    for(int i=0;i<n;i++)
    {
        if(v[i]<=k)
            count++;
        else
        break;
    }
    cout<<count;
    return 0;
}
